{
  "name": "react-typescript-redux",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "typings install",
    "build": "webpack --progress --colors --watch --config ./webpack.config.dev.js",
    "build:prod": "webpack --config ./webpack.config.prod.js",
    "server": "node dev-server.es6",
    "test:ts": "mocha --require ts-node/register \"./**/*-test.ts\"",
    "test:tsx": "mocha --require ts-node/register ./test-setup.es6 \"./**/*-test.tsx\"",
    "test:ut": "mocha --require ts-node/register ./test-setup.es6",
    "test:coverage": "tsc && npm run testWithCoverage",
    "coveralls": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json && istanbul report lcov && node mergeCoveralls.es6 < ./coverage/lcov.info",
    "test:local-coverage": "npm run test:coverage && remap-istanbul -i ./coverage/coverage.json -o coverage-report -t html",
    "testWithCoverage": "istanbul cover _mocha -- ./test-setup.es6 ./dist/**/*-test.js"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.6.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "express": "^4.14.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "remap-istanbul": "^0.6.4",
    "sinon": "^1.17.4",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.9.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "object-assign": "^4.1.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
